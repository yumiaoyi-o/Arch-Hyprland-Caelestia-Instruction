    "editorInfo.foreground": "#fde4e0",
    
    "diffEditor.insertedTextBackground": "#f7b7b533",
    "diffEditor.removedTextBackground": "#95474d33",
    
    "terminal.background": "#1d1c20",
    "terminal.foreground": "#fde4e0",
    "terminal.ansiBlack": "#1d1c20",
    "terminal.ansiRed": "#95474d",
    "terminal.ansiGreen": "#9a7170",
    "terminal.ansiYellow": "#f7b7b5",
